<?xml version="1.0" encoding="utf-8"?>
<bundle name="PublishPress Revisions Pro">
  <domain name="revisionary-pro">
    <project name="PublishPress Revisions Pro" slug="revisionary-pro">
      <source>
        <directory>includes-pro</directory>
      </source>
      <target>
        <directory>includes-pro/languages</directory>
      </target>
      <template>
        <file>includes-pro/languages/revisionary-pro.pot</file>
      </template>
    </project>
  </domain>
  <domain name="revisionary">
    <project name="PublishPress Revisions" slug="revisionary">
      <source>
        <directory>.</directory>
      </source>
      <target>
        <directory>languages</directory>
      </target>
      <template>
        <file>languages/revisionary.pot</file>
      </template>
    </project>
  </domain>
  <exclude>
    <directory>includes-pro</directory>
    <directory>vendor</directory>
  </exclude>
</bundle>
