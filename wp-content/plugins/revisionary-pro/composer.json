{
  "name": "publishpress/publishpress-revisions-pro",
  "type": "wordpress-plugin",
  "description": "",
  "homepage": "https://publishpress.com/",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "PublishPress",
      "email": "help@publishpress.com"
    },
    {
      "name": "Kevin Behrens",
      "email": "kevin@agapetry.net",
      "homepage": "https://agapetry.net",
      "role": "Lead Developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2.5"
  },
  "scripts": {
    "build": "ppbuild build",
    "build:dir": "ppbuild build-dir",
    "build:clean": "ppbuild clean",
    "get-version": "ppbuild version",
    "gen:mo": "wp i18n make-mo ./languages ./languages --allow-root",
    "gen:pot": "wp i18n make-pot . languages/revisionary-pro.pot --domain=revisionary-pro --exclude=dev-workspace,.wordpress-org,.github,dist,tests, --allow-root",
    "check:longpath": "longpath .",
    "check:deps": "composer check:deps --working-dir=./lib",
    "fix:deps": "composer fix:deps --working-dir=./lib",
    "pre-autoload-dump": "composer dumpautoload --working-dir=./lib",
    "pre-update-cmd": "composer update --working-dir=./lib",
    "pre-install-cmd": "composer install --working-dir=./lib"
  },
  "extra": {
    "plugin-slug": "revisionary-pro",
    "plugin-name": "publishpress-revisions-pro",
    "plugin-folder": "revisionary-pro"
  }
}
